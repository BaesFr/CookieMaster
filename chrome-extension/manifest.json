{
  "name": "CookieMaster - Cookie Clicker Plugin",
  "short_name": "CookieMaster",
  "version": "1.11.3",
  "manifest_version": 2,
  "description": "A feature-rich plugin for Cookie Clicker",
  "homepage_url": "http://cookiemaster.co.uk",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    "lib/ui+en.css",
    "lib/jquery-2.1.0.min.js",
    "lib/jsapi.js",
    "lib/corechart.js",
    "lib/uds.js",
    "build/cookiemaster.css",
    "build/external-methods.js",
    "build/cookiemaster.js"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://orteil.dashnet.org/cookieclicker/"
      ],
      "js": [
        "inject.js"
      ]
    }
  ]
}